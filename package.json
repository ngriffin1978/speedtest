    "express-rate-limit": "^7.1.5",
    "helmet": "^8.1.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "eslint": "^8.55.0",
    "jest": "^30.2.0",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
