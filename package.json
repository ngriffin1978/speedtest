{
  "name": "sdwan-speedtest",
  "version": "1.0.0",
  "description": "SD-WAN Network Speed Test Server - Docker-based performance testing for SD-WAN environments",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --coverage",
